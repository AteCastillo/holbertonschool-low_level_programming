#include "holberton.h"
/**
 * main - prints the environment
 *
 * Return: Always 0.
 */

int main(void)
{
	_printenv();
	return (0);
}

/**
 *
 * _printenv - to print the env
void _printenv(void)
{
	extern char **environ;
	int i;
	size_t j;

	for (i = 0; environ[i] != 0; i++)
	{
		for (j = 0; environ[i][j] != 0; j++)
		{
		}
		write(1, environ[i], j);
		write(1, "\n", 1);
	}
}
