#include <stdio.h>
#include "function_pointers.h"

/**
 * print_elem - prints an integer
 * @array: the integer to print
 * @size: size
 * @action: pointer to function
 * Return: Nothing.
 */

void array_iterator(int *array, size_t size, void (*action)(int))

{
	int n;
	if (array == NULL)
		return;
	for (n = 0; n < size; n++)
		(*action)(array[n]);

}
